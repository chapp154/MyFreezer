{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch localhost",
            "type": "chrome",
            "request": "launch",
            "url": "http://127.0.0.1:8080/dist/",
            "pathMappings": [
                {
                    "url": "webpack://myfreezer/src",
                    "path": "${workspaceFolder}/src"
                },
                {
                    "url": "http://127.0.0.1:8080",
                    "path": "${workspaceFolder}"
                }
            ]
        }
    ]
}